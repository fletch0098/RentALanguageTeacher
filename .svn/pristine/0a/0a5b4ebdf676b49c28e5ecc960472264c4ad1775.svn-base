/*Menu Elements*/

/*Main Menu*/

.MainMenu
{
    /*    Main Menu        */
    height: 20px;
    text-align: center;
} 

.MainMenu ul li
{
    /*
    Gives the six menu items in the main menu a width of 100px each.
  */
    width: 90px;
    margin: auto;
    text-align: center;

}

 ul.level1
{
    /* Defines the appearance of main menu items. */
    font-size: 1.6em;
    color: #ffffff;


    height: 20px;
    line-height: 20px;
    vertical-align: text-bottom;
    text-align: left;
    margin-right: auto;
    margin-left: auto;
}

 ul.level1 .selected {
    /* Active menu items font colo black and orange background */
    background-color: #edac1c;
    color: #000000;
}

 a.level1
{
  /* Adds some white space to the left of the main menu item text.
!important is used to overrule the in-line CSS that the menu generates */
  padding-left: 5px !important;
}

 a.level2
{
  /* Defines the appearance of the sub menu items. */
  background-color: #555555;
  padding-left: 8px;
}

 a.level1:hover, a.level2:hover {
    /* hover style for the main and sub items. Orange background and black font*/
    background-color: #edac1c;
    color: #000000;
}

.FooterMenu
{
    /*
    Menu fits container
  */
    width: 400px;
    height: 25px;
    text-align:center;

} 

.FooterMenu ul li
{
    /*
    Gives the five menu items in the main menu a width of 70px each and center
  */
    width: 80px;
margin:auto;
    text-align: center;
    font-size: 1.3em;
}
.FooterMenu ul.level1
{
    text-align:center;
        margin-right: auto;
    margin-left: auto;
}

.FooterMenu ul.level1 .selected {
    /* Active menu items font colo black and orange background */
    background:none;
    color: #edac1c;
}

.FooterMenu a.level1
{
  /* Adds some white space to the left of the main menu item text.
!important is used to overrule the in-line CSS that the menu generates */
  padding-left: 5px !important;
}

.FooterMenu a.level2
{
  /* Defines the appearance of the sub menu items. */
  background-color: #555555;
  padding-left: 8px;
}

.FooterMenu a.level1:hover, a.level2:hover {
    /* hover style for the main and sub items. Orange background and black font*/

        background:none;
    color: #edac1c;
}

